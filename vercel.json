{
  "version": 2,
  "builds": [
    {
      "src": "/backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "/frontend/build",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { 
        "src": "/favicon.ico", 
        "dest": "/frontend/build/favicon.ico" 
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/backend/index.js"
    }
  ]
}
